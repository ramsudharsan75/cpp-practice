    "version": "2.0.0",
    "tasks": [
        {
            "type": "cppbuild",
            "label": "Template Project: gcc build",
            "command": "g++-12",
            "args": [
                "-g",
                "-std=c++20",
                "${workspaceFolder}/TemplateProject/*.cpp",
                "-o",
                "${workspaceFolder}/TemplateProject/rooster"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
        },
